{
  "trailingComma": "all",
  "singleQuote": true,
  "printWidth": 140,
  "tabWidth": 4,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/components/(.*)$",
    "^@/lib/(.*)$",
    "^@/features/(.*)$",
    "^@/types/(.*)$",
    "^./(.*)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
}
